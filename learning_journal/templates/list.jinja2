{% extends 'base.jinja2' %}
{% block content%}
    <article class="entries">
        {% for entry in entries %}
        <!-- <a href="{{ entry.id }}">{{ entry.title }}</a> -->
        <a href="{{ request.route_url('entry_route', entry_id=entry.id) }}">{{ entry.title }}</a>
        <p>{{ entry.created }}</p>
        {% endfor %}
    </article>
{% endblock %}
